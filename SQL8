UPDATE employee
SET name = 'Marry'
WHERE id = 1;

UPDATE employee
SET name = 'Jones'
WHERE name = 'Marry';

UPDATE employee
SET email = 'ffind1@gmail.com'
WHERE email = 'find@gmail.com;

UPDATE employee
SET name = 'Alice'
WHERE id = 2;

UPDATE employee
SET name = 'Cara'
WHERE id = 3;

DELETE FROM employee
WHERE name = 'Cara';

DELETE FROM employee
WHERE name = 'Alice';

DELETE FROM employee
WHERE email = 'ffind1@gmail.com';

DELETE FROM employee
WHERE id = '1';

